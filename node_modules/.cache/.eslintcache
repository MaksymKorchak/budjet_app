set' :{\n            return defaultContext\n        };\n        default: \n            throw new Error('No Action');\n    }\n}\n\nconst AppContextProvider = (props) => {\n    const [state, dispatch] = useReducer(reducer, defaultContext);\n    const value = { state, dispatch };\n\n    return (\n        <AppContext.Provider value={value}>\n           {props.children}\n        </AppContext.Provider>\n    )\n}\n\nexport {\n    AppContext,\n    AppContextProvider\n}\n","/Users/maksym/Desktop/budjet_app/src/components/App/styles.js",[],"/Users/maksym/Desktop/budjet_app/src/utils/indexdb.js",[],"/Users/maksym/Desktop/budjet_app/src/providers/context/defaultContext.js",["245"],"/Users/maksym/Desktop/budjet_app/src/components/Home/index.js",[],"/Users/maksym/Desktop/budjet_app/src/components/Statistics/index.js",[],"/Users/maksym/Desktop/budjet_app/src/components/Header/index.js",[],"/Users/maksym/Desktop/budjet_app/src/components/About/index.js",[],"/Users/maksym/Desktop/budjet_app/src/components/Settings/index.js",["246","247","248"],"import { useContext, useMemo, useState } from \"react\";\nimport { AppContext } from \"../../providers/context\";\nimport { useBooleanToggle } from \"../../hooks\";\nimport { LOCALES } from \"../../providers/i18n\";\nimport { FormattedMessage } from \"react-intl\";\nimport { saveToStorage } from \"../../utils/sessionStorage\";\n\nconst Settings = () => {\n\n    const {state, dispatch} = useContext(AppContext);\n    const {status, handleStatus} = useBooleanToggle(false);\n\n    const onChangeCurrency = (e) => {\n        const { value } = e.target;\n        dispatch({\n            type: 'changeCurrency',\n            currency: value\n        })\n    };\n\n    const onChangeLocale = (e) => {\n        const { value } = e.target;\n        dispatch({\n            type: 'setLocale',\n            locale: value\n        })\n        saveToStorage('locale', value);\n    };    \n\n    const data = useMemo(() => 'Hello') ;\n\n    return (\n        <>\n            <div>\n                <form>\n                    <h2>\n                        <FormattedMessage id=\"currency_ssettings\"/>\n                    </h2>\n                    <div>\n                        <label>\n                            <FormattedMessage id=\"currency\"/> \n                            <select \n                                onChange = {onChangeCurrency}\n                                name=\"currency\" \n                                value={state.currency}\n                            >\n                                <option value=\"USD\">USD</option>\n                                <option value=\"EUR\">EUR</option>\n                                <option value=\"UAH\">UAH</option>\n                            </select>\n                        </label>\n                    </div>\n                    <h2>\n                        <FormattedMessage id=\"language_ssettings\"/>\n                    </h2>\n                    <div>\n                        <label>\n                            <FormattedMessage id=\"language\"/> \n                            <select \n                                onChange = {onChangeLocale}\n                                name=\"locale\" \n                                value={state.locale}>\n                                <option value={LOCALES.UKRAINIAN}>UKR</option>\n                                <option value={LOCALES.ENGLISH}>ENG</option>\n                            </select>\n                        </label>\n                    </div>\n                </form>\n            </div>\n            <div>\n                <button onClick={handleStatus}>\n                    <FormattedMessage id=\"advanced_ssettings\"/>\n                </button>\n                {status ? (\n                    <div>\n                        <h2>\n                            <FormattedMessage id=\"advanced_ssettings\"/>\n                        </h2>\n                        <p>...</p>\n                    </div>\n                ) : null }\n\n            </div>\n       </>\n    )\n};\n\nexport default Settings; ","/Users/maksym/Desktop/budjet_app/src/hooks.js",["249"],"import { useState, useEffect, useCallback } from 'react';\n\nimport { STATUSES } from './constants';\nimport { getData, addItem, deleteItem, updateItem } from './utils/indexdb'\n\nexport const useBooleanToggle = (initialStatus = false) => {\n    const [status, setStatus] = useState(initialStatus);\n\n    const handleStatusChange = () => {\n        console.log('switch state');\n        setStatus((currrentStatus) => !currrentStatus);\n    };\n\n    return {\n        status,\n        handleStatusChange\n    }\n}\n\nexport const useData = () => {\n    const [state, setState] = useState({\n        transactions: [],\n        error: '',\n        status: STATUSES.IDLE,\n        hasNextPage: true,\n        balance: 0,\n    });\n\n    const setBalance = (transaction) => {\n        setState((state) => ({\n            ...state,\n            balance: state.balance + Number(transaction.value)\n        }));\n    };\n\n    useEffect(() => {\n        setState({\n            ...state,\n            status: STATUSES.PENDING\n        });\n\n        getData().then((transactions) => {\n            setState({\n                ...state,\n                transactions,\n                status: STATUSES.SUCCESS,\n                hasNextPage: true,    \n            })\n            transactions.forEach((transaction) => setBalance(transaction))\n        }).catch((e) => {\n            setState({\n                ...state,\n                transactions: [],\n                status: STATUSES.ERROR,\n                error: e,\n                hasNextPage: false\n            })\n        })\n    }, [])\n\n    const loadMoreRows = useCallback(() => {\n        setState({\n            ...state,\n            status: STATUSES.PENDING\n        });\n\n        getData(state.transactions.length, 20).then((transactions) => {\n            setState({\n                ...state,\n                transactions: [...state.transactions, ...transactions],\n                status: STATUSES.SUCCESS\n            })\n            transactions.forEach((transaction) => setBalance(transaction))\n        }).catch(() => {\n            setState({\n                ...state,\n                hasNextPage: false\n            })\n        })\n    }, [state])\n\n    const pushTransaction = useCallback((data) => {\n\n        const transaction = {\n            ...data,\n            value: +data.value,\n            id: Date.now()\n        }\n\n        setState((state) => ({\n            ...state,\n            balance: state.balance +  Number(transaction.value),\n            transactions: [transaction, ...state.transactions]\n        }));\n\n        addItem(transaction);\n\n    }, [setState]);\n\n    const onDelete = useCallback((id) => {\n        const item = state.transactions.find((i) => i.id === id);\n\n        setState((state) => ({\n            ...state,\n            balance: state.balance - Number(item.value),\n            transactions: state.transactions.filter((item) => item.id !== id)\n        }));\n\n        deleteItem(id);\n    }, [setState,state]);\n\n    const onStarClick = useCallback((id) => {\n        const item = state.transactions.find((i) => i.id === id);\n        \n        updateItem({\n            ...item,\n            isStarred: !item.isStarred\n        }).then(() => {\n            setState((state) => ({\n                ...state,\n                transactions: state.transactions.map((item) => item.id !== id ? item : {\n                    ...item,\n                    isStarred: !item.isStarred\n                })\n            }))\n        })\n        \n    }, [setState, state])\n\n    return {\n        ...state,\n        pushTransaction,\n        onDelete,\n        onStarClick,\n        loadMoreRows\n    }\n}","/Users/maksym/Desktop/budjet_app/src/components/Header/styles.js",[],"/Users/maksym/Desktop/budjet_app